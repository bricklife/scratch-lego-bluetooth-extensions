{
  "name": "scratch-lego-bluetooth-extensions",
  "version": "0.1.0",
  "description": "Scratch 3.0 extensions for LEGO Bluetooth and BLE devices",
  "scripts": {
    "register": "npm run register:spikeprime && npm run register:legoble && npm run register:spikeessential && npm run register:legoremote && npm run register:controlplus && npm run register:poweredup && npm run register:duplotrain && npm run register:legoluigi && npm run register:legomario && npm run register:iftttWebhooks && npm run register:speechChrome && npm run register:boostextra",
    "register:spikeprime": "node ./scripts/register.js --link --id=\"spikeprime\" --block=\"./scratch-vm/src/extensions/scratch3_spikeprime\" --entry=\"./scratch-gui/src/lib/libraries/extensions/spikeprime\" --gui=\"../scratch-gui\"",
    "register:legoble": "node ./scripts/register.js --link --id=\"legoble\" --block=\"./scratch-vm/src/extensions/scratch3_legoble\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoble\" --gui=\"../scratch-gui\"",
    "register:spikeessential": "node ./scripts/register.js --link --id=\"spikeessential\" --block=\"./scratch-vm/src/extensions/scratch3_spikeessential\" --entry=\"./scratch-gui/src/lib/libraries/extensions/spikeessential\" --gui=\"../scratch-gui\"",
    "register:legoremote": "node ./scripts/register.js --link --id=\"legoremote\" --block=\"./scratch-vm/src/extensions/scratch3_legoremote\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoremote\" --gui=\"../scratch-gui\"",
    "register:controlplus": "node ./scripts/register.js --link --id=\"controlplus\" --block=\"./scratch-vm/src/extensions/scratch3_controlplus\" --entry=\"./scratch-gui/src/lib/libraries/extensions/controlplus\" --gui=\"../scratch-gui\"",
    "register:poweredup": "node ./scripts/register.js --link --id=\"poweredup\" --block=\"./scratch-vm/src/extensions/scratch3_poweredup\" --entry=\"./scratch-gui/src/lib/libraries/extensions/poweredup\" --gui=\"../scratch-gui\"",
    "register:duplotrain": "node ./scripts/register.js --link --id=\"duplotrain\" --block=\"./scratch-vm/src/extensions/scratch3_duplotrain\" --entry=\"./scratch-gui/src/lib/libraries/extensions/duplotrain\" --gui=\"../scratch-gui\"",
    "register:legomario": "node ./scripts/register.js --link --id=\"legomario\" --block=\"./scratch-vm/src/extensions/scratch3_legomario\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legomario\" --gui=\"../scratch-gui\"",
    "register:legoluigi": "node ./scripts/register.js --link --id=\"legoluigi\" --block=\"./scratch-vm/src/extensions/scratch3_legoluigi\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoluigi\" --gui=\"../scratch-gui\"",
    "register:iftttWebhooks": "node ./scripts/register.js --link --id=\"iftttWebhooks\" --block=\"./scratch-vm/src/extensions/scratch3_iftttWebhooks\" --entry=\"./scratch-gui/src/lib/libraries/extensions/iftttWebhooks\" --gui=\"../scratch-gui\"",
    "register:speechChrome": "node ./scripts/register.js --link --id=\"speechChrome\" --block=\"./scratch-vm/src/extensions/scratch3_speechChrome\" --entry=\"./scratch-gui/src/lib/libraries/extensions/speechChrome\" --gui=\"../scratch-gui\"",
    "register:boostextra": "node ./scripts/register.js --link --id=\"boostextra\" --block=\"./scratch-vm/src/extensions/scratch3_boostextra\" --entry=\"./scratch-gui/src/lib/libraries/extensions/boostextra\" --gui=\"../scratch-gui\"",
    "register:tmpose2scratch": "node ./scripts/register.js --link --id=\"tmpose2scratch\" --block=\"./scratch-vm/src/extensions/scratch3_tmpose2scratch\" --entry=\"./scratch-gui/src/lib/libraries/extensions/tmpose2scratch\" --gui=\"../scratch-gui\"",
    "build": "npm run build:spikeprime && npm run build:legoble && npm run build:spikeessential && npm run build:legoremote && npm run build:controlplus && npm run build:poweredup && npm run build:duplotrain && npm run build:legoluigi && npm run build:legomario && npm run build:iftttWebhooks && npm run build:speechChrome && npm run build:boostextra",
    "build:spikeprime": "npx xcratch-build --module=\"spikeprime\" --block=\"scratch-vm/src/extensions/scratch3_spikeprime\" --entry=\"scratch-gui/src/lib/libraries/extensions/spikeprime\" --gui=\"../scratch-gui\"",
    "build:legoble": "npx xcratch-build --module=\"legoble\" --block=\"scratch-vm/src/extensions/scratch3_legoble\" --entry=\"scratch-gui/src/lib/libraries/extensions/legoble\" --gui=\"../scratch-gui\"",
    "build:spikeessential": "npx xcratch-build --module=\"spikeessential\" --block=\"scratch-vm/src/extensions/scratch3_spikeessential\" --entry=\"scratch-gui/src/lib/libraries/extensions/spikeessential\" --gui=\"../scratch-gui\"",
    "build:legoremote": "npx xcratch-build --module=\"legoremote\" --block=\"scratch-vm/src/extensions/scratch3_legoremote\" --entry=\"scratch-gui/src/lib/libraries/extensions/legoremote\" --gui=\"../scratch-gui\"",
    "build:controlplus": "npx xcratch-build --module=\"controlplus\" --block=\"scratch-vm/src/extensions/scratch3_controlplus\" --entry=\"scratch-gui/src/lib/libraries/extensions/controlplus\" --gui=\"../scratch-gui\"",
    "build:poweredup": "npx xcratch-build --module=\"poweredup\" --block=\"scratch-vm/src/extensions/scratch3_poweredup\" --entry=\"scratch-gui/src/lib/libraries/extensions/poweredup\" --gui=\"../scratch-gui\"",
    "build:duplotrain": "npx xcratch-build --module=\"duplotrain\" --block=\"scratch-vm/src/extensions/scratch3_duplotrain\" --entry=\"scratch-gui/src/lib/libraries/extensions/duplotrain\" --gui=\"../scratch-gui\"",
    "build:legomario": "npx xcratch-build --module=\"legomario\" --block=\"scratch-vm/src/extensions/scratch3_legomario\" --entry=\"scratch-gui/src/lib/libraries/extensions/legomario\" --gui=\"../scratch-gui\"",
    "build:legoluigi": "npx xcratch-build --module=\"legoluigi\" --block=\"scratch-vm/src/extensions/scratch3_legoluigi\" --entry=\"scratch-gui/src/lib/libraries/extensions/legoluigi\" --gui=\"../scratch-gui\"",
    "build:iftttWebhooks": "npx xcratch-build --module=\"iftttWebhooks\" --block=\"scratch-vm/src/extensions/scratch3_iftttWebhooks\" --entry=\"scratch-gui/src/lib/libraries/extensions/iftttWebhooks\" --gui=\"../scratch-gui\"",
    "build:speechChrome": "npx xcratch-build --module=\"speechChrome\" --block=\"scratch-vm/src/extensions/scratch3_speechChrome\" --entry=\"scratch-gui/src/lib/libraries/extensions/speechChrome\" --gui=\"../scratch-gui\"",
    "build:boostextra": "npx xcratch-build --module=\"boostextra\" --block=\"scratch-vm/src/extensions/scratch3_boostextra\" --entry=\"scratch-gui/src/lib/libraries/extensions/boostextra\" --gui=\"../scratch-gui\"",
    "build:tmpose2scratch": "npx xcratch-build --module=\"tmpose2scratch\" --block=\"scratch-vm/src/extensions/scratch3_tmpose2scratch\" --entry=\"scratch-gui/src/lib/libraries/extensions/tmpose2scratch\" --gui=\"../scratch-gui\"",
    "start": "cd ../scratch-gui && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bricklife/scratch-lego-bluetooth-extensions.git"
  },
  "keywords": [
    "scratch",
    "lego",
    "bluetooth",
    "ble"
  ],
  "author": "Shinichiro Oba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bricklife/scratch-lego-bluetooth-extensions/issues"
  },
  "homepage": "https://github.com/bricklife/scratch-lego-bluetooth-extensions",
  "devDependencies": {
    "xcratch-build": "github:bricklife/xcratch-build#lego-bluetooth-extensions"
  }
}
